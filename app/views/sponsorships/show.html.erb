<%= link_to 'Back', sponsorships_path %>

<p id="notice"><%= notice %></p>

<div class="sponsorship show">
    <h2>Show Sponsorship</h2>

    <div class="actions">
        <%= link_to("Edit", edit_sponsorship_path(@sponsorship), class: 'action edit') %>
        <%= link_to("Delete", delete_sponsorship_path(@sponsorship), class: 'action delete') %>
    </div>

    <table summary="Sponsorship detail view">
        <tr>
            <th>Sponsor Name</th>
            <td><%= @sponsorship.sponsor_name %></td>
        </tr>
        <tr>
            <th>Lead Name</th>
            <td><%= @sponsorship.sponsor_lead_name %></td>
        </tr>
        <tr>
            <th>Phone</th>
            <td><%= @sponsorship.sponsor_phone %></td>
        </tr>
        <tr>
            <th>Email</th>
            <td><%= @sponsorship.sponsor_email %></td>
        </tr>
        <tr>
            <th>Donation</th>
            <td><%= @sponsorship.sponsor_donation %></td>
        </tr>
        <tr>
            <th>End of Contract</th>
            <td><%= @sponsorship.sponsor_end_of_contract %></td>
        </tr>
    </table>
</div>
