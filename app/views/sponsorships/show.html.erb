<p id="notice"><%= notice %></p>

<div class="sponsorship show">
    <h2>Show sponsorship</h2>

    <div class="actions">
        <%= link_to("Edit", edit_sponsorship_path(@sponsorship), class: 'action edit') %>
        <%= link_to("Delete", delete_sponsorship_path(@sponsorship), class: 'action delete') %>
    </div>

    <table summary="Sponsorship detail view">
        <tr>
            <th>Sponsor Name</th>
            <td><%= @sponsorship.sponsorName %></td>
        </tr>
        <tr>
            <th>Lead Name</th>
            <td><%= @sponsorship.sponsorLeadName %></td>
        </tr>
        <tr>
            <th>Phone</th>
            <td><%= @sponsorship.sponsorPhone %></td>
        </tr>
        <tr>
            <th>Email</th>
            <td><%= @sponsorship.sponsorEmail %></td>
        </tr>
        <tr>
            <th>Donation</th>
            <td><%= @sponsorship.sponsorDonation %></td>
        </tr>
        <tr>
            <th>End of Contract</th>
            <td><%= @sponsorship.sponsorEndOfContract %></td>
        </tr>
    </table>
</div>
